package controleur;

import java.util.*;

import modele.DemandeDeLivraison;
import modele.Plan;

import vue.FenetreIHM;

/**
 * 
 */
public class EtatInit extends EtatDefaut {

    /**
     * Default constructor
     */
    public EtatInit() {
    }

    /**
     * @param fenetre
     */
    public Boolean ouvrirPlan(Plan plan) {
        return plan.chargerPlan();
    }
    
    public void importerLivraison(FenetreIHM fenetre,DemandeDeLivraison demandeDeLivraison, Plan plan) {
        fenetre.afficheMessage("Veuillez charger un plan !");
    }

}